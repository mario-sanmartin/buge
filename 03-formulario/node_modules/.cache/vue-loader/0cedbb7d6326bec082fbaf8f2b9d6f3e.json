/components/ListarTareas.vue","sourceRoot":"","sourcesContent":["<template>\n  <table class=\"table\">\n    <thead>\n        <tr>\n        <th scope=\"col\">#</th>\n        <th scope=\"col\">Nombre</th>\n        <th scope=\"col\">Categorias</th>\n        <th scope=\"col\">estado</th>\n        <th scope=\"col\">Numero</th>\n        <th scope=\"col\">Acciones</th>\n        </tr>\n    </thead>\n    <tbody>\n        <tr v-for=\"item in tareas\" :key=\"item.id\">\n            <th>{{item.id}}</th>\n            <td>{{item.nombre}}</td>\n            <td>\n                <span v-for=\"(cat,index) in item.categoria\" :key=\"index\">\n                    {{\n                        item.categoria.length === index + 1 ? cat : cat +', '\n                        \n                    }}\n                </span>\n            </td>\n            <td>{{item.estado}}</td>\n            <td>{{item.numero}}</td>\n            <td>\n                <button class=\"btn btn-danger btn-sm\" @click=\"deleteTarea(item.id)\">\n                    Eliminar\n                </button>\n                <router-link\n                class=\"btn btn-warning ml-2 btn-sm\"\n                :to=\"{\n                    name:'Editar',\n                    params:{\n                        id: item.id\n                    }\n                    }\"\n                >\n                    Editar\n                </router-link>\n            </td>\n        </tr>\n  </tbody>\n</table>\n</template>\n\n<script>\nimport {mapState ,mapActions} from 'vuex';\nexport default {\n    computed:{\n        ...mapState(['tareas'])\n    },\n    methods: {\n        ...mapActions(['deleteTarea'])\n    },\n\n\n}\n</script>"]}]}